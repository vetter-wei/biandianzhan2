<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_40">

        <TextView
            android:id="@+id/tv_pjrw_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dimen_13"
            android:text="断路器评价细则任务"
            android:textColor="@color/font_4d4d4d"
            android:textSize="@dimen/textsize_17" />

        <ImageView
            android:id="@+id/iv_renwu_fragment_row"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dimen_21"
            android:src="@mipmap/row_right" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginLeft="@dimen/dimen_13"
        android:layout_marginRight="@dimen/dimen_13"
        android:background="@drawable/dash_line"
        android:layerType="software" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_24">

        <TextView
            android:id="@+id/tv_renwu_fragment_start"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dimen_13"
            android:text="开始时间"
            android:textColor="@color/font_7a7a7a"
            android:textSize="@dimen/textsize_14" />

        <TextView
            android:id="@+id/tv_pjrw_startTime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dimen_9"
            android:layout_toRightOf="@+id/tv_renwu_fragment_start"
            android:text="2016/11/09 19:54"
            android:textColor="@color/font_6d6d6d"
            android:textSize="@dimen/textsize_14" />

        <TextView
            android:id="@+id/tv_renwu_fragment_end"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dimen_9"
            android:layout_toLeftOf="@+id/tv_pjrw_endTime"
            android:text="结束时间"
            android:textColor="@color/font_7a7a7a"
            android:textSize="@dimen/textsize_14" />

        <TextView
            android:id="@+id/tv_pjrw_endTime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dimen_13"
            android:text="2016/11/09 19:54"
            android:textColor="@color/font_6d6d6d"
            android:textSize="@dimen/textsize_14" />


    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_10"
        android:background="@color/color_f2f2f2" />
</LinearLayout>
